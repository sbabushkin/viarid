{
  "name": "viar-id",
  "version": "^1.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:schema": "nest build && yarn emit:schema",
    "prepare": "yarn build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "DEBUG=knex* nest start",
    "start:dev": "IS_LOCAL=true nest start --watch",
    "start:debug": "IS_LOCAL=true nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "IN_MEMORY_PUBSUB=true jest",
    "test:watch": "IN_MEMORY_PUBSUB=true jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:rollback": "cd ./src && knex migrate:rollback",
    "db:rollback:logs": "cd ./src && DB_SCOPE=logs knex migrate:rollback",
    "db:migrate": "cd ./src && knex migrate:latest",
    "db:migrate:logs": "cd ./src && DB_SCOPE=logs knex migrate:latest",
    "db:make": "cd ./src && knex migrate:make",
    "db:make:logs": "cd ./src && DB_SCOPE=logs knex migrate:make",
    "test:ci": "NODE_ENV=test yarn run db:migrate && IN_MEMORY_PUBSUB=true jest --silent",
    "emit:schema": "cross-env PORT=3001 ts-node ./src/tools/emitSchema.ts"
  },
  "dependencies": {
    "@google-cloud/local-auth": "^2.1.1",
    "@graphile-contrib/pg-omit-archived": "^2.1.0",
    "@graphile-contrib/pg-simplify-inflector": "^6.1.0",
    "@graphile/pg-aggregates": "^0.1.0",
    "@graphile/postgis": "^0.1.0",
    "@nestjs/common": "^7.6.18",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.18",
    "@nestjs/graphql": "^7.11.0",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/mapped-types": "^0.4.1",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.18",
    "@nestjs/swagger": "6.0.1",
    "@nestjs/testing": "^7.6.18",
    "@types/lodash": "^4.14.170",
    "@types/nodemailer": "^6.4.2",
    "@types/passport": "^1.0.6",
    "@willsoto/nestjs-objection": "^5.0.0",
    "apollo-server-express": "^2.25.2",
    "aws-sdk": "^2.933.0",
    "class-transformer": "0.4.0",
    "class-validator": "^0.13.1",
    "cross-env": "^7.0.3",
    "date-fns": "^2.23.0",
    "debug": "^4.3.4",
    "dotenv": "^9.0.0",
    "exceljs": "^4.3.0",
    "form-data": "^4.0.0",
    "googleapis": "105",
    "graphile-utils": "^4.12.1",
    "graphql": "^15.5.1",
    "graphql-scalars": "^1.17.0",
    "graphql-subscriptions": "^1.2.1",
    "graphql-tools": "^7.0.4",
    "graphql-upload": "^12.0.0",
    "ioredis": "^4.28.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.19",
    "knex-postgis": "^0.13.0",
    "libphonenumber-js": "^1.9.44",
    "lodash": "^4.17.21",
    "nest-module-redis": "^0.1.0",
    "node-html-parser": "^6.1.5",
    "node-xlsx": "^0.21.0",
    "nodemailer": "^6.6.2",
    "objection": "^2.2.18",
    "parse-price": "^1.1.8",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.6.0",
    "postgraphile": "^4.12.3",
    "postgraphile-apollo-server": "^0.1.1",
    "postgraphile-plugin-connection-filter": "^2.2.1",
    "punycode": "^2.1.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.7",
    "sharp": "^0.30.2",
    "smartcrop-sharp": "^2.0.5",
    "supertest": "^6.1.3",
    "transliteration": "^2.2.0",
    "typestate": "^1.0.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^7.6.18",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.14.44",
    "@types/supertest": "^2.0.11",
    "jest": "^26.6.3",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.5",
    "ts-loader": "^8.2.0",
    "ts-node": "10.8.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.6.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
